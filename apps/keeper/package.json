{
  "name": "@primodiumxyz/keeper",
  "version": "0.0.1",
  "description": "Keeper API for Primodium games",
  "repository": {
    "type": "git",
    "url": "https://github.com/primodiumxyz/keeper.git",
    "directory": "."
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/src/index.js"
  },
  "types": "src/index.ts",
  "bin": {
    "keeper-server": "./dist/bin/keeper-server.js"
  },
  "scripts": {
    "build": "pnpm run build:js",
    "build:js": "tsup",
    "clean": "pnpm run clean:js",
    "clean:js": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "start": "tsx bin/keeper-server",
    "test": "tsc --noEmit --skipLibCheck",
    "test:ci": "pnpm run test"
  },
  "dependencies": {
    "@fastify/compress": "^6.5.0",
    "@fastify/cors": "^8.3.0",
    "@primodiumxyz/core": "workspace:*",
    "@trpc/client": "10.34.0",
    "@trpc/server": "10.34.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "fastify": "^4.21.0",
    "viem": "2.9.20",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "@types/debug": "^4.1.7",
    "prettier": "^3.3.2",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "vitest": "0.31.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
